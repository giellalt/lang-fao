REMOVE Neu IF (0 (Pron Dem Gen))(*1 (N Fem Gen) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Dem Dat))(*1 (N Msc Dat) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Dem Dat))(*1 (N Fem Dat) BARRIER NPNHA LINK NOT 0 Neu);

REMOVE Msc IF (0 (Pron Dem Nom))(*1 (N Fem Nom) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Nom))(*1 (N Neu Nom) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Acc))(*1 (N Fem Acc) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Acc))(*1 (N Neu Acc) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Gen))(*1 (N Fem Gen) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Gen))(*1 (N Neu Gen) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Dat))(*1 (N Fem Dat) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Dem Dat))(*1 (N Neu Dat) BARRIER NPNHA LINK NOT 0 Msc);

REMOVE Fem IF (0 (Pron Dem Nom))(*1 (N Msc Nom) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Nom))(*1 (N Neu Nom) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Acc))(*1 (N Msc Acc) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Acc))(*1 (N Neu Acc) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Gen))(*1 (N Msc Gen) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Gen))(*1 (N Neu Gen) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Dat))(*1 (N Msc Dat) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Dem Dat))(*1 (N Neu Dat) BARRIER NPNHA LINK NOT 0 Fem);


REMOVE Neu IF (0 (Pron Det Nom))(*1 (N Msc Nom) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Nom))(*1 (N Fem Nom) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Acc))(*1 (N Msc Acc) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Acc))(*1 (N Fem Acc) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Gen))(*1 (N Msc Gen) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Gen))(*1 (N Fem Gen) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Dat))(*1 (N Msc Dat) BARRIER NPNHA LINK NOT 0 Neu);
REMOVE Neu IF (0 (Pron Det Dat))(*1 (N Fem Dat) BARRIER NPNHA LINK NOT 0 Neu);

REMOVE Msc IF (0 (Pron Det Nom))(*1 (N Fem Nom) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Nom))(*1 (N Neu Nom) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Acc))(*1 (N Fem Acc) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Acc))(*1 (N Neu Acc) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Gen))(*1 (N Fem Gen) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Gen))(*1 (N Neu Gen) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Dat))(*1 (N Fem Dat) BARRIER NPNHA LINK NOT 0 Msc);
REMOVE Msc IF (0 (Pron Det Dat))(*1 (N Neu Dat) BARRIER NPNHA LINK NOT 0 Msc);

REMOVE Fem IF (0 (Pron Det Nom))(*1 (N Msc Nom) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Det Nom))(*1 (N Neu Nom) BARRIER NPNHA LINK NOT 0 Fem);
REMOVE Fem IF (0 (Pron Det Acc))(*1 (N Msc Acc) BARRIER NPNHA LINK NOT 0 Fem);
