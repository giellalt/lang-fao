# Case disambiguation
SELECT Nom IF (0 (Pron Dem))(*1C Nom BARRIER NPNHA);
SELECT Acc IF (0 (Pron Dem))(*1C Acc BARRIER NPNHA);
SELECT Dat IF (0 (Pron Dem))(*1C Dat BARRIER NPNHA);
SELECT Gen IF (0 (Pron Dem))(*1C Gen BARRIER NPNHA);
						  
REMOVE Nom IF (0 (Pron Dem))(*1 N    BARRIER NPNHA LINK NOT 0 Nom);
REMOVE Acc IF (0 (Pron Dem))(*1 N    BARRIER NPNHA LINK NOT 0 Acc);
REMOVE Dat IF (0 (Pron Dem))(*1 N    BARRIER NPNHA LINK NOT 0 Dat);
REMOVE Gen IF (0 (Pron Dem))(*1 N    BARRIER NPNHA LINK NOT 0 Gen);

SELECT Nom IF (0 (Pron Det))(*1C Nom BARRIER NPNHA);
SELECT Acc IF (0 (Pron Det))(*1C Acc BARRIER NPNHA);
SELECT Dat IF (0 (Pron Det))(*1C Dat BARRIER NPNHA);
SELECT Gen IF (0 (Pron Det))(*1C Gen BARRIER NPNHA);
						  
REMOVE Nom IF (0 (Pron Det))(*1 N    BARRIER NPNHA LINK NOT 0 Nom);
REMOVE Acc IF (0 (Pron Det))(*1 N    BARRIER NPNHA LINK NOT 0 Acc);
REMOVE Dat IF (0 (Pron Det))(*1 N    BARRIER NPNHA LINK NOT 0 Dat);
REMOVE Gen IF (0 (Pron Det))(*1 N    BARRIER NPNHA LINK NOT 0 Gen);


# Adjective disambiguation
