 +Pl+Dat:%>u DF_D_PL ;

LEXICON F_PLGEN
 +Pl+Gen:%>a DF_G_PL ;
 +Pl+Gen:%>a R ; ! gen compound


! Neuter forms
! ____________

LEXICON S_N_SGNA
