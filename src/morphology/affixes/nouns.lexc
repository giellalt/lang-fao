!! __@LEXNAME@__ for -ur-
 +Pl+Acc:%>ur DF_A_PLm ;

LEXICON W_M_PLDAT
!! __@LEXNAME@__ for -u-
 +Pl+Dat:%>u DF_D_PL ;

LEXICON W_M_PLGEN
!! __@LEXNAME@__ for -a-
 +Pl+Gen:%>a DF_G_PL ;




!! !Strong case suffixes
! ____________________


!! Nominative Sg

LEXICON S_M_SGNOM
 +Sg+Nom:%>ur  DF_N_SGm_indef ;
 +Sg+Nom:%>uri DF_N_SGm_def   ;

LEXICON S_M_SGNOM_NULL
 +Sg+Nom: DF_N_SGm ;
 +Sg+Nom: DF_N_SGm ;


!! Accusative Sg

LEXICON S_M_SGACC
 +Sg+Acc:    DF_A_SGm_indef ; !sand
 +Sg+Acc:%>i DF_A_SGm_def   ; !sandin
 +Sg+Acc:    R ; ! stem compound

!! Dative Sg

LEXICON S_M_SGDAT
 +Sg+Dat:%>i DF_D_SGm ;

LEXICON S_M_SGDAT_2
     +Sg+Dat:%>i          DF_D_SGm ;
 +Sg+Dat+Def:^GVDEL%>num ENDLEX        ;

LEXICON S_M_SGDAT_NULL
 +Sg+Dat: DF_D_SGm ;

! Genitive Sg

LEXICON S_M_SGGEN
 +Sg+Gen:%>s DF_G_SGm ;
 +Sg+Gen:%>s R ; ! gen compound

LEXICON S_M_SGGEN_NULL
 +Sg+Gen: DF_G_SGm ;
 +Sg+Gen: R ; ! gen compound

LEXICON S_M_SGGEN_AR
 +Sg+Gen:%>ar DF_G_SGm ;
 +Sg+Gen:%>ar R ; ! gen compound

!! Plural forms

!! Nominative

LEXICON S_M_PLNOM
 +Pl+Nom:%>ar DF_N_PLm ;

LEXICON S_M_PLNOM_IR
 +Pl+Nom:%>ir DF_N_PLm ;

LEXICON S_M_PLNOM_UR
 +Pl+Nom:%>ur DF_N_PLm ;

