 +N+Msc:     W_M_SGGEN ;
 +N+Msc:     W_M_PLNOM ;
 +N+Msc:     W_M_PLACC ;
 +N+Msc:     W_M_PLDAT ;
 +N+Msc:     W_M_PLGEN ;

