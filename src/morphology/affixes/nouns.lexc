LEXICON S_M_PLACC_NULL_NULL
  +Pl+Acc:     DF_A_PLm_indef ;
  +Pl+Acc:     DF_A_PLm_def   ;


! Dative

LEXICON S_M_PLDAT
 +Pl+Dat:%>u DF_D_PL ;

LEXICON S_M_PLDATm ! skóm
 +Pl+Dat: DF_D_PL ;

! Genitive

LEXICON S_M_PLGEN
 +Pl+Gen:%>a DF_G_PL ;
 +Pl+Gen:%>a R       ; ! gen pl compound


