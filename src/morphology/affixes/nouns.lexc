 +Pl+Nom:    DF_N_PLm_indef ;
 +Pl+Nom:%>i DF_N_PLm_def   ;

LEXICON S_M_PLNOM_NULL_NULL
  +Pl+Nom:     DF_N_PLm_indef ;
  +Pl+Nom:     DF_N_PLm_def   ;
  

! Accusative

LEXICON S_M_PLACC
 +Pl+Acc:%>ar DF_A_PLm ;

