       +Pl+Acc:%>i  DF_NA_PLn_W ;
 +Pl+Nom+Indef:%>ir ENDLEX       ;
 +Pl+Acc+Indef:%>ir ENDLEX       ;

LEXICON N_PLNA
 +Pl+Nom: DF_NA_PLn ;
 +Pl+Acc: DF_NA_PLn ;


