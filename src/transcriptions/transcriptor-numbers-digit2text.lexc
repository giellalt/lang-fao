göökte:2   CUODI ;
golme:3    CUODI ;
njieljie:4 CUODI ;
njielje+Use/NG:4  CUODI ;
nieljie+Use/NG:4  CUODI ;
nielje+Use/NG:4  CUODI ;
vïjhte:5   CUODI ;
govhte:6   CUODI ;
tjïjhtje:7 CUODI ;
gaektsie:8 CUODI ;
uktsie:9   CUODI ;

